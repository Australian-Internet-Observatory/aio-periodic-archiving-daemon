{
  "name": "periodicArchivingDaemon",
  "version": "1.0.0",
  "description": "The Periodic Archiving Daemon empowers web-archiving research, by enabling participants to use their web browsers to periodically archive websites.",
  "scripts": {
    "chrome-build": "cross-env TARGET=chrome gulp",
    "opera-build": "cross-env TARGET=opera gulp",
    "firefox-build": "cross-env TARGET=firefox gulp",
    "buildmv2": "cross-env NODE_ENV=production NODE_MV=2 npm run opera-build && cross-env NODE_ENV=production NODE_MV=2 npm run firefox-build",
    "buildmv3": "cross-env TARGET=chrome NODE_ENV=development NODE_MV=3 gulp build",
    "chrome-watch": "cross-env TARGET=chrome gulp watch",
    "opera-watch": "cross-env TARGET=opera gulp watch",
    "firefox-watch": "cross-env TARGET=firefox gulp watch",
    "chrome-dist": "cross-env NODE_ENV=production cross-env TARGET=chrome gulp dist",
    "opera-dist": "cross-env NODE_ENV=production cross-env TARGET=opera gulp dist",
    "firefox-dist": "cross-env NODE_ENV=production cross-env TARGET=firefox gulp dist",
    "dist": "npm run chrome-dist && npm run opera-dist && npm run firefox-dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Australian-Internet-Observatory/aio-periodic-archiving-daemon"
  },
  "author": "Abdul Karim Obeid <aisjerfd@gmail.com> (https://github.com/askoj)",
  "bugs": {
    "url": "https://github.com/Australian-Internet-Observatory/aio-periodic-archiving-daemon/issues"
  },
  "homepage": "https://internetobservatory.org.au/periodic-archiving-daemon",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/register": "^7.14.5",
    "babel-core": "^6.26.3",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chokidar": "^3.6.0",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.3",
    "deasync": "^0.1.21",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-livereload": "^4.0.2",
    "gulp-load-plugins": "^2.0.7",
    "gulp-merge-json": "^2.1.1",
    "gulp-plumber": "^1.2.1",
    "gulp-print": "^5.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.0.1",
    "gulp-zip": "^5.1.0",
    "minify-stream": "^2.1.0",
    "preprocessify": "^1.0.1",
    "run-sequence": "^2.2.1",
    "through2": "^4.0.2",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchpack": "^2.4.4",
    "webpack": "^4.46.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "esm": "^3.2.25",
    "gulp-connect": "^5.7.0",
    "math": "^0.0.3",
    "mhtml2html": "3.0.0",
    "moment-timezone": "^0.5.33",
    "python2": "0.0.1",
    "random-id": "^1.0.4"
  },
  "overrides": {
    "node-gyp": "^11.4.1"
  }
}
